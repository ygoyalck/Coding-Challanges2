/**********
 * Following is the Node class that is already written.
 
class Node{
public:
    int data;
    Node *next;
    Node(int data){
        this -> data = data;
        this -> next = NULL;
    }
};
 
*********/

int indexOfNIter(Node *head, int n) {
  Node* temp=head;
  int i=0;
for(temp=head;n>0 && temp!=NULL;temp=temp->next)
{
  if(temp->data==n)
    return i;
  i++;
  //n--;
}
  if(temp==NULL)
    return -1;
  //else if(temp->data==n)
    
}


